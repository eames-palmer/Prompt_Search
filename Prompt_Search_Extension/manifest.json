{
    "manifest_version": 2,
    "name": "Global Search",
    "version": "0.1",
    "permissions": [
        "activeTab"
    ],
    "commands": {
        "toggle-feature-foo": {
            "suggested_key": {
                "default": "Alt+Shift+A"
            },
            "description": "Toggle feature foo"
        }
    },
    "background": {
        "matches": [
            "<all_urls>"
        ],
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    }
}