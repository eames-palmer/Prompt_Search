{
    "manifest_version": 2,
    "name": "Prompt Search",
    "version": "0.2",
    "permissions": [
        "activeTab"
    ],
    "background": {
        "matches": [
            "<all_urls>"
        ],
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Prompt Search: Set Keybinding in chrome://extensions/shortcuts"
    }
}